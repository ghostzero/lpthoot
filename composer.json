{
    "name": "ghostzero/lpthoot",
    "description": "A simple library to poll the streamers status instead of using webhooks.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "René Preuß",
            "email": "rene@preuss.io"
        }
    ],
    "require": {
        "ext-json": "*",
        "laravel/framework": "5.8.*|^7.0",
        "orchestra/testbench": "^3.7|^5.0",
        "phpunit/phpunit": "^7.5|^8.5",
        "romanzipp/laravel-twitch": "^2.0",
        "firebase/php-jwt": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "GhostZero\\LPTHOOT\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GhostZero\\LPTHOOT\\Providers\\PollServiceProvider"
            ]
        }
    }
}
